+				<prop actor="structures/celts/skiff.xml" attachpoint="root" />
       </props>
       <textures>
         <texture file="structural/celt_struct.dds" name="baseTex"/>
diff --git a/binaries/data/mods/public/art/actors/structures/iberians/shipyard.xml b/binaries/data/mods/public/art/actors/structures/iberians/shipyard.xml
